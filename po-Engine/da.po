msgid "Engine Commands"
msgstr "Motorkommandoer"

#: ../src/Engine/Session.cs:510
#, csharp-format
msgid "Disconnect failed - could not find server: {0}"
msgstr "Afbrydelse fejlede - kunne ikke finde server: {0}"

