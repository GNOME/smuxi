<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkBox" id="EngineAssistantIntroWidget">
    <property name="can_focus">False</property>
    <child>
      <object class="GtkLabel" id="label2">
        <property name="can_focus">False</property>
        <property name="xalign">0</property>
        <property name="label" translatable="yes">Welcome to the Smuxi Engine Configuration Assistant.
You need to enter some information before you can use the engine.

Click "Continue" to begin.</property>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
        <property name="position">0</property>
      </packing>
    </child>
  </object>
</interface>
